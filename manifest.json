{
  "manifest_version": 2,

  "name": "test",
  "description": "ex",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "index.html"
  },

  "permissions": [
    "tabs", "http://*/*"
  ]
}